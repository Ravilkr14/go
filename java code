class food{
    String dishes;
    String dessert;
    int cost;
    
    public void menu(){
        System.out.println(this.dishes);
        System.out.println(this.dessert);
        System.out.println(this.cost);
    }
}

public class Main
{
	public static void main(String[] args){ 
	food food1 = new food();
	food1.dishes = "dbc";
	food1.dessert = "gulabjamun";
	food1.cost = 100;
	food1.menu();  
	}
	
	
}

//polymorphism
class Dish{
    String maincourse;
    int cost;
    
    public void printInfo(String maincourse){
        System.out.println(maincourse); 
    }
    public void printInfo(int cost){
        System.out.println(cost);
    }
    
}

public class Main
{
	public static void main(String[] args) {
	    Dish d1= new Dish();
	    d1.maincourse="aalu ke parathe";
	    d1.cost=89;
	    
	    d1.printInfo(d1.cost);
		
	}
}
